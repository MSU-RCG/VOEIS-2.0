%h3Query Results
For: Variable(
=variable
), Site(
=site
)
-if !start_date.nil?
  , Review(
  -if start_date > end_date
    ERROR - start date should not come after end date
  -else
    =start_date
    ="-" 
    = end_date
  ) 
=clear_break
%table
  %tr
    -column_array.each do |col|
      %th
        =col[0]
  -row_array.each do |row|
    %tr
      -row.each do |col|
        %td
          =col

-if row_array.empty?
  Sorry - there were no results for this search.
-else
  -visualization "my_table", "Table", :width => 1000, :height => 400, :html => {:class => "g_table"} do |chart|
    -column_array.each do |col|
      -if col[0] == "Timestamp"
        -chart.datetime "Timestamp"
      -elsif col[1] == 'number'
        -chart.number col[0]
    -chart.add_rows(row_array)