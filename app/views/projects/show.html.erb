<h1><%= @project.name %></h1>
<table id="projects-show">
  <thead>
    <tr>
      <th>Models</th>
      <th>Data</th>
    </tr>
  </thead>
  <tbody>
    <tr id="projects-show-row<%=@project.id%>" class="projects-show-row-border">
      <td><%= render :partial => 'yogo_models/model_list' %></td>
      <td><%= render :partial => 'yogo_data/data_list' %></td>
    </tr>
  </tbody>
</table>
<%= render :partial => 'upload' %>

<%= link_to("Remove Project", project_path(@project), :method => :delete, :confirm => "Removing this project will destroy all data associated with it.\n Continue?") %>
