- visualization("#{project.id.to_s}", 'map', { :enableScrollWheel => true, :showTip => true, :useMapTypeControl => true, :html => { :class => class_name } }) do |map|
  - map.register_column(:number, :Lat)
  - map.register_column(:number, :Lon)
  - map.register_column(:string, :Name)
  - Rails.logger.debug { "**************** There are #{sites.count} sites. *************" }
  - map.add_rows(sites.collect{|site| [site.latitude, site.longitude, site.name] })
