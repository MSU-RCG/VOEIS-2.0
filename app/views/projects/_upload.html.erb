<h1>Data File Upload</h1>
<% form_tag({:id => @project.id, :action => 'upload_csv'}, :multipart => true) do -%>
    <p><label for="upload_file">Select File</label> : 
    <%= file_field 'upload', 'datafile', :accept => "text/csv;text/comma-separated-values" %></p>
    <%= submit_tag "Upload" %>
<% end -%>
