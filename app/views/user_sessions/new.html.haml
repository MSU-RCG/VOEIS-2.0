
.extra-small-centered
  %h1#content-title Log In
  = clear_break
  .rbox.dashed
    #user-form
      - form_for( :user_session, :url => user_session_path, :builder => YogoFormBuilder ) do |f|
        = f.error_messages
        = f.text_field(:login, :label => "User Name:")
        = f.password_field(:password, :label => 'Password:')
        = f.check_box(:remember_me)
        = f.submit("Login")
