<H3><%= @model.name.split("::")[-1].titleize %></H3>
<table id="data-show">
  <thead>
    <tr>
      <th></th><th></th>
      <%- @model.properties.each do |h| %>
      <th><%= h.name.to_s.titleize %></th>
      <%- end %>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= link_to(image_tag('edit.png')+'Edit', edit_project_yogo_data_path(@project, @model.name.split("::")[-1], @item.id)) %></td>
      <td><%= link_to(image_tag('remove.png')+'Delete', project_yogo_data_path(@project, @model.name.split("::")[-1], @item.id), :method => :delete, :confirm => "There is no undo.\n Continue?") %></td>
      <%- @model.properties.each do |p| %>
      <td><%= @item[p.name] %></td>
      <%- end %>
    </tr>
  </tbody>
</table>

